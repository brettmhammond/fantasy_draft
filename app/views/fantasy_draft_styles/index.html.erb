<h1>Listing fantasy_draft_styles</h1>

<table class="table table-bordered">
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fantasy_draft_styles.each do |fantasy_draft_style| %>
      <tr>
        <td><%= fantasy_draft_style.name %></td>
        <td><%= link_to 'Show', fantasy_draft_style %></td>
        <td><%= link_to 'Edit', edit_fantasy_draft_style_path(fantasy_draft_style) %></td>
        <td><%= link_to 'Destroy', fantasy_draft_style, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fantasy draft style', new_fantasy_draft_style_path %>
